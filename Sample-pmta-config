#####################################################################################################################################################################################################################
# Section 5: SMTP PATTERN SETTINGS ##################################################################################################################################################################################
#####################################################################################################################################################################################################################

<smtp-pattern-list common-errors>
  reply /generating high volumes of.* complaints from AOL/    mode=normal
  reply /Excessive unknown recipients - possible Open Relay/  mode=normal
  reply /^421 .* too many errors/                             mode=normal
  reply /blocked.*spamhaus/                                   mode=normal
  reply /451 Rejected/                                        mode=normal
</smtp-pattern-list>

<smtp-pattern-list blocking-errors>
    #
    # A QUEUE IN BACKOFF MODE WILL SEND MORE SLOWLY
    # To place a queue back into normal mode, a command similar
    # to one of the following will need to be run:
    # pmta set queue --mode=normal yahoo.com
    # or
    # pmta set queue --mode=normal yahoo.com/vmta1
    #
    # To use backoff mode, uncomment individual <domain> directives
    #
  #AOL Errors
    reply /421 .* SERVICE NOT AVAILABLE/ mode=normal
    reply /generating high volumes of.* complaints from AOL/ mode=normal
    reply /554 .*aol.com/ mode=normal
    reply /421dynt1/ mode=normal
    reply /HVU:B1/ mode=normal
    reply /DNS:NR/ mode=normal
    reply /RLY:NW/ mode=normal
    reply /DYN:T1/ mode=normal
    reply /RLY:BD/ mode=normal
    reply /RLY:CH2/ mode=normal
    #
    #Yahoo Errors
    reply /421 .* Please try again later/ mode=normal
    reply /421 Message temporarily deferred/ mode=normal
    reply /VS3-IP5 Excessive unknown recipients/ mode=normal
    reply /VSS-IP Excessive unknown recipients/ mode=normal
    #
    # The following 4 Yahoo errors may be very common
    # Using them may result in high use of backoff mode
    #
    reply /\[GL01\] Message from/ mode=normal
    reply /\[TS01\] Messages from/ mode=normal
    reply /\[TS02\] Messages from/ mode=normal
    reply /\[TS03\] All messages from/ mode=normal
    #
    #Hotmail Errors
    reply /exceeded the rate limit/ mode=normal
    reply /exceeded the connection limit/ mode=normal
    reply /Mail rejected by Windows Live Hotmail for policy reasons/ mode=normal
    reply /mail.live.com\/mail\/troubleshooting.aspx/ mode=normal
    #
    #Adelphia Errors
    reply /421 Message Rejected/ mode=normal
    reply /Client host rejected/ mode=normal
    reply /blocked using UCEProtect/ mode=normal
    #
    #Road Runner Errors
    reply /Mail Refused/ mode=normal
    reply /421 Exceeded allowable connection time/ mode=normal
    reply /amIBlockedByRR/ mode=normal
    reply /block-lookup/ mode=normal
    reply /Too many concurrent connections from source IP/ mode=normal
    #
    #General Errors
    reply /too many/ mode=normal
    reply /Exceeded allowable connection time/ mode=normal
    reply /Connection rate limit exceeded/ mode=normal
    reply /refused your connection/ mode=normal
    reply /try again later/ mode=normal
    reply /try later/ mode=normal
    reply /550 RBL/ mode=normal
    reply /TDC internal RBL/ mode=normal
    reply /connection refused/ mode=normal
    reply /please see www.spamhaus.org/ mode=normal
    reply /Message Rejected/ mode=normal
    reply /refused by antispam/ mode=normal
    reply /Service not available/ mode=normal
    reply /currently blocked/ mode=normal
    reply /locally blacklisted/ mode=normal
    reply /not currently accepting mail from your ip/ mode=normal
    reply /421.*closing connection/ mode=normal
    reply /421.*Lost connection/ mode=normal
    reply /476 connections from your host are denied/ mode=normal
    reply /421 Connection cannot be established/ mode=normal
    reply /421 temporary envelope failure/ mode=normal
    reply /421 4.4.2 Timeout while waiting for command/ mode=normal
    reply /450 Requested action aborted/ mode=normal
    reply /550 Access denied/ mode=normal
    reply /exceeded the rate limit/ mode=normal
    reply /421rlynw/ mode=normal
    reply /permanently deferred/ mode=normal
    reply /\d+\.\d+\.\d+\.\d+ blocked/ mode=normal
    reply /www\.spamcop\.net\/bl\.shtml/ mode=normal
    reply /generating high volumes of.* complaints from AOL/    mode=normal
    reply /Excessive unknown recipients - possible Open Relay/  mode=normal
    reply /^421 .* too many errors/                             mode=normal
    reply /blocked.*spamhaus/                                   mode=normal
    reply /451 Rejected/                                        mode=normal
</smtp-pattern-list>

############################################################################
# END Section 5: SMTP PATTERN SETTINGS #####################################
############################################################################

############################################################################
# Section 6: LOG AND SPOOL SETTINGS ########################################
############################################################################

log-file /var/log/pmta/log        # logrotate is used for rotation

# All logs
 <acct-file /var/log/pmta/acct.csv>
     move-interval 5m
     max-size 50M
     delete-after 7d
 </acct-file>

