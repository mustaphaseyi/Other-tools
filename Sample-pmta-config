<domain iua.queue>
# Routing directive
#    max-msg-rate 10000/h
#    max-cold-virtual-mta-msg 1000000/d
#    backoff-to-normal-after 10m              # added in v3.5
#    backoff-max-msg-rate 10000/h              # Use with PowerMTA 3.5
#    retry-after 10m
#    backoff-retry-after 10m
#    max-smtp-out 2
#    max-msg-per-connection 50
# Normail directive   
    smtp-hosts lookup-mx:i.ua
    max-smtp-out 5
    max-rcpt-per-message 10
    smtp-pattern-list blocking-errors
    retry-after 10m
    max-msg-rate 300/h
    backoff-to-normal-after 30m             # added in v3.5
    backoff-max-msg-rate 100/h              # Use with PowerMTA 3.5
    backoff-retry-after 30m
    # backoff-notify admin@mydomain.com
    dk-sign yes
    dkim-sign yes
    deliver-local-dsn no
</domain>     

