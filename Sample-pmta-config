# BEGIN: OTHER OPTIONS #####################################################
############################################################################

sync-msg-create false
sync-msg-update false
run-as-root no

#
# spool directories
#

<spool /var/spool/pmta>
    deliver-only no
    delete-file-holders yes
</spool>

